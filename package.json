{
  "name": "react-rubik",
  "version": "1.0.0",
  "description": "React Rubik is a small project with a big purpose. Create a set of useful components for future React Projects.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.babel.js",
    "build:prd": "webpack -p --config config/webpack.config.babel.js",
    "build": "webpack --config config/webpack.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codesandtags/react-rubik.git"
  },
  "keywords": [
    "react",
    "webpack",
    "redux",
    "ava",
    "babel",
    "eslint",
    "susy"
  ],
  "author": "codesandtags",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codesandtags/react-rubik/issues"
  },
  "homepage": "https://github.com/codesandtags/react-rubik#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "object-assign": "^4.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "v7.0.0-alpha.2",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.6.3",
    "file-loader": "0.9.0",
    "highlightjs": "^9.10.0",
    "json-loader": "0.5.4",
    "node-sass": "^4.5.0",
    "postcss-loader": "1.2.1",
    "sass-loader": "^6.0.3",
    "style-loader": "0.13.1",
    "susy": "^2.2.12",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react",
      "stage-1"
    ],
    "env": {
      "development": {
        "presets": [
          "latest"
        ]
      },
      "production": {
        "presets": [
          [
            "latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "latest"
        ]
      }
    }
  }
}
